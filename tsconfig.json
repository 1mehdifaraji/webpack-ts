  "compilerOptions": {
    "noImplicitAny": true,
    "target": "ES5",
    "module": "ES2020",
    "sourceMap": false,
    "moduleResolution": "nodenext",
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": true,
    "emitDeclarationOnly": true,
    "declaration": true
  }
}
